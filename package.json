{
    "name": "evenity",
    "version": "1.0.0",
    "scripts": {
        "dfx_install": "sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "setup": "npm i --force && dfx canister create events && dfx generate events && dfx deploy",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy events",
        "canister_deploy_mainnet": "dfx deploy --network ic events",
        "canister_uninstall": "dfx canister uninstall-code events",
        "start": "run-p frontend backend",
        "frontend": "vite --port 3000",
        "backend": "mo-dev --generate --deploy -y",
        "deploy": "dfx deploy azle_events",
        "build": "tsc && vite build",
        "test": "run-s test:backend test:frontend",
        "test:frontend": "vitest run",
        "test:backend": "mo-test",
        "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\" \"backend/**/*.mo\"",
        "sources": "mops sources",
        "postinstall": "mops install"
    },
    "dependencies": {
        "@auth0/auth0-react": "^2.0.1",
        "@dfinity/agent": "^0.20.2",
        "@dfinity/auth-client": "^0.20.2",
        "@dfinity/candid": "^0.20.2",
        "@dfinity/identity": "^0.20.2",
        "@dfinity/principal": "^0.20.2",
        "@emotion/styled": "^11.10.6",
        "@tippyjs/react": "^4.2.6",
        "qrcode": "^1.5.3",
        "qrcode.react": "^3.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-dropzone": "^14.2.3",
        "react-icons": "^4.8.0",
        "react-markdown": "^8.0.6",
        "react-modal": "^3.16.1",
        "react-router-dom": "^6.9.0",
        "react-select": "^5.7.2",
        "react-toastify": "^9.1.2",
        "recharts": "^2.8.0",
        "styled-components": "^5.3.9",
        "sweetalert2": "^11.4.8",
        "turndown": "^7.1.2",
        "twin.macro": "^3.1.0",
        "usehooks-ts": "^2.9.1",
        "zustand": "^4.3.6"
    },
    "devDependencies": {
        "@emotion/react": "^11.10.6",
        "@heroicons/react": "^2.0.18",
        "@tailwindcss/typography": "^0.5.9",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/qrcode": "^1.5.5",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/react-lottie": "^1.2.10",
        "@types/react-modal": "^3.16.3",
        "@types/styled-components": "^5.1.26",
        "@types/turndown": "^5.0.1",
        "@vitejs/plugin-react": "^3.1.0",
        "autoprefixer": "^10.4.14",
        "azle": "0.18.6",
        "babel-plugin-twin": "^1.1.0",
        "cross-env": "^7.0.3",
        "cross-fetch": "^3.1.5",
        "dotenv": "^16.3.1",
        "esbuild": "^0.17.12",
        "eslint": "^8.36.0",
        "eslint-config-react-app": "^7.0.1",
        "husky": "^8.0.3",
        "ic-mops": "^0.9.1",
        "jsdom": "^21.1.1",
        "lint-staged": "^13.2.0",
        "mo-dev": "^0.9.0",
        "node-fetch": "^3.3.2",
        "npm-run-all": "^4.1.5",
